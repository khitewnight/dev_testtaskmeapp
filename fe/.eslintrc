{
	"extends": "airbnb",
	"env": {"browser": true},
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"linebreak-style": 0,
		"react/forbid-prop-types": 0,
		"no-unused-vars": 0
	},
	"settings": {
		"import/resolver": {
			"node": {
				"moduleDirectory": ["node_modules", "src/"]
			}
		}
	},
	"parser": "babel-eslint"
}
